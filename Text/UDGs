10 for i=1 to 63:
    read a$:
    FOR a=udg "a" TO UDG "a"+63 step 8
20       READ user:
       for b=7 to 0 step -1:
          POKE fontbank,a+7-b,(user & (1 shl b))<>0:
       next b
30    NEXT a
40    x=even i*20: print tab x;a$;":";tab 14+x;"á  ";
50 next i

