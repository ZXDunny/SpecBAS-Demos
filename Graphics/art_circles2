ZXASCII
AUTO -1
PROG /demos/Graphics/art_circles2
CHANGED FALSE
10 over 1:ink 1:palette 9,0: degrees:
	A=int(rnd*10)+2,B=int(rnd*13)+3:
	FOR J=1 TO B:
	   FOR I=1 TO A:
       circle 400+60*COS(360/A*I)+100*COS(360/B*J),240+60*SIN(360/A*I)+100*SIN(360/B*J),70 fill:
				NEXT I:
	NEXT J:
	pause 0:
	run
