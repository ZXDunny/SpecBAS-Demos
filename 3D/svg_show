ZXASCII
AUTO -1
PROG demos:3d/svg_show
CHANGED FALSE
10 stream new s,"demos:3d/models/r2d2":
 stream read s,f$,streamlen s:
 stream close s
20 do:
    read s$,r$:
    f$=replace$(f$,s$,r$):
 loop until s$=""
30 daTa "M ;","",#13,"",#10,""," ","",";",": WAIT SCREEN ","L",
      ": DRAW TO ","M",": MOVE TO ","END","","",""
40 w=16000:
 origin -w/8,0 to (w/8)*7,w*scrh/scrw flip:
 EXECUTE f$(3 to )
