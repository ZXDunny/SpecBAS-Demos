ZXASCII
AUTO -1
PROG /demos/Graphics/metatron
CHANGED FALSE
10 rem Metatron's cube
20 dim p(13,2): sw=scrw/2,sh=scrh/2,cr=53
30 p(1,1)=sw,p(1,2)=sh,i=2
 
40 for r=cr*2 to cr*4 step cr*2:
    for a=0 to 5:
       p(i,1)=sw+cos(a*tau/6)*r,
       p(i,2)=sh+sin(a*tau/6)*r:
       i+=1:
    next a:
 next r

100 for i=1 to 13:
    circle p(i,1),p(i,2),cr:
    for j=i+1 to 13:
       draw p(i,1),p(i,2) to p(j,1),p(j,2)
110    next j:
 next i
