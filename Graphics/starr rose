ZXASCII
AUTO -1
PROG starr rose
10 REM Starr Rose
20 LET a=7,b=18,c=8: RADIANS: SCREEN LOCK 
30 FOR r=MIN(SCRw,SCRh)/5 TO 50 STEP -2
40 FOR i=0 TO 2*PI STEP .01: LET k=2+SIN(a*i)/2,tt=i+SIN(b*i)/c,x=(r*k*COS tt)+SCRw/2,y=-(r*k*SIN tt)+SCRh/2: IF i=0 THEN LET ox=x,oy=y: PLOT x,y ELSE DRAW TO x,y
50 NEXT i: NEXT r
70 LET c=6+MOUSEx/10,b=MOUSEy/10: WAIT SCREEN: CLS: GO TO 30
