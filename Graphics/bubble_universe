ZXASCII
AUTO -1
PROG demos:graphics/bubble_universe
CHANGED FALSE
10 const n=200,r=tau/235:
 x,y,v,t=0,sz=200,sw=scrw/sz,sh=scrh/sz:
 window depth 0,32:
 origin -sw,-sh to sw,sh:
 screen lock:
 do:
    cls 0:
    for i=0 to n;j=0 to n:
       u=sin(i+v)+sin(r*i+x),
       v=cos(i+v)+cos(r*i+x),
       x=u+t:
       plot ink rgbtoint(i,j,99);u,v:
    next j;i:
    t+=.025:
    wait screen:
 loop
