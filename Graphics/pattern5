ZXASCII
AUTO -1
PROG /demos/Graphics/pattern5
CHANGED FALSE
10 DIM S(50) base 0:
	degrees:
	sc=3:
	origin -400,-240 to 400,240:
	FOR L=0 TO 49:
	   S(L)=SIN(L*9):
	NEXT L:
	FOR L=20 TO 1010*SC/2:
	   O=L-40:
				P=L MOD 40:
				Q=(L+1) MOD 40:
				S=S(P)/SC:
				C=S(P+10)/SC:
				T=S(Q)/SC:
				D=S(Q+10)/SC:
				IF RND>.5 then
							DRAW L*S,L*C to O*T,O*D:
				ELSE
				   draw L*T,L*D to O*S,O*C:
				end if:
	NEXT L
